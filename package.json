{
  "private": true,
  "name": "botbuilder-js",
  "version": "4.12.0",
  "workspaces": [
    "libraries/*",
    "libraries/functional-tests/dialogToDialog/*",
    "libraries/testskills/*",
    "testing/*",
    "tools",
    "transcripts"
  ],
  "scripts": {
    "browser-functional-test": "cd libraries/browser-functional-tests && node nightwatch.js -e",
    "build": "yarn execute --script build --silent",
    "build-docs": "yarn execute --script build-docs --silent",
    "clean": "yarn execute --script clean --silent",
    "eslint": "eslint libraries/*/src/**/*.ts --resolve-plugins-relative-to .",
    "eslint:fix": "yarn eslint --fix",
    "functional-test": "yarn build && nyc mocha \"libraries/functional-tests/tests/*.test.js\"",
    "package": "yarn execute --no-private --silent yarn pack",
    "test": "yarn build && nyc mocha \"libraries/@(adaptive*|bot*)/tests/**/*.test.js\" --exit --check-leaks",
    "test:compat": "yarn execute --script test:compat --silent",
    "test:consumer": "yarn workspace consumer-test start",
    "test:coveralls": "yarn build && nyc mocha \"libraries/@(adaptive*|bot*)/tests/**/*.test.js\" --exit && nyc report --reporter=text-lcov | coveralls",
    "test:devops": "nyc --report --reporter=cobertura mocha -- \"libraries/@(adaptive*|bot*)/tests/**/*.test.js\" --check-leaks --reporter mocha-junit-reporter --reporter-options includePending=true",
    "execute": "yarn workspace botbuilder-repo-utils execute",
    "update-versions": "yarn workspace botbuilder-repo-utils update-versions"
  },
  "devDependencies": {
    "@azure/ms-rest-js": "1.9.0",
    "@microsoft/api-extractor": "^7.7.12",
    "@types/jsonwebtoken": "7.2.8",
    "@types/lodash": "^4.14.134",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "applicationinsights": "^1.7.5",
    "coveralls": "^3.0.4",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jsdoc": "^29.2.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-security": "^1.4.0",
    "mocha": "^6.2.3",
    "mocha-junit-reporter": "^2.0.0",
    "ms-rest-azure": "^2.6.0",
    "prettier": "^2.1.2",
    "read-text-file": "^1.1.0",
    "replace-in-file": "^4.1.0",
    "sinon": "^7.5.0",
    "typedoc": "^0.15.0",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "typedoc-plugin-markdown": "^2.2.10"
  },
  "nyc": {
    "exclude": [
      "**/botframework*/**/generated/**",
      "**/botbuilder*/**/generated/**",
      "**/bot-integration-tests/**",
      "**/adaptive-expressions/**/generated/**",
      "**/botframework-luis/**",
      "**/tests/**",
      "**/tools/**",
      "**/internal.*"
    ]
  }
}
