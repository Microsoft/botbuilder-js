{
    "$schema": "../../../../botbuilder-dialogs-adaptive/tests/schema/test.schema",
    "$kind": "Microsoft.Test.Script",
    "enableTrace": true,
    "dialog": {
        "$kind": "Microsoft.AdaptiveDialog",
        "id": "planningTest",
        "triggers": [
            {
                "$kind": "Microsoft.OnUnknownIntent",
                "actions": [
                    {
                        "$kind": "Teams.GetPagedTeamMembers",
                        "property": "$result"
                    },
                    {
                        "$kind": "Microsoft.TraceActivity",
                        "name": "sendResult",
                        "valueType": "memory",
                        "value": "=$result"
                    }
                ]
            }
        ]
    },
    "script": [
        {
            "$kind": "Microsoft.Test.UserActivity",
            "activity": {
                "type": "message",
                "text": "hi",
                "channelData": {
                    "tenant": {
                        "id": "tenant-id-1"
                    },
                    "team": {
                        "id": "team-id-1"
                    }
                }
            }
        },
        {
            "$kind": "Microsoft.Test.AssertReplyActivity",
            "assertions": [
                "value.continuationToken == 'token'",
                "value.members[0].email == 'User.0@microsoft.com'",
                "value.members[0].id == '29:User-Id-0'",
                "value.members[0].objectId == 'User-0-Object-Id'",
                "value.members[1].email == 'User.1@microsoft.com'",
                "value.members[1].id == '29:User-Id-1'",
                "value.members[1].objectId == 'User-1-Object-Id'",
                "value.members[2].email == 'User.2@microsoft.com'",
                "value.members[2].id == '29:User-Id-2'",
                "value.members[2].objectId == 'User-2-Object-Id'"
            ]
        }
    ]
}