{
    "$schema": "../../../../botbuilder-dialogs-adaptive/tests/schema/test.schema",
    "$kind": "Microsoft.Test.Script",
    "dialog": {
        "$kind": "Microsoft.AdaptiveDialog",
        "id": "planningTest",
        "triggers": [
            {
                "$kind": "Microsoft.OnUnknownIntent",
                "actions": [
                    {
                        "$kind": "Teams.GetMeetingParticipant",
                        "property": "user.result"
                    }
                ]
            }
        ]
    },
    "script": [
        {
            "$kind": "Microsoft.Test.UserActivity",
            "activity": {
                "type": "message",
                "text": "hi",
                "channelData": {
                    "tenant": {
                        "id": "tenant-id-1"
                    },
                    "meeting": {
                        "id": "meeting-id-1"
                    }
                }
            }
        },
        {
            "$kind": "Microsoft.Test.MemoryAssertions",
            "assertions": [
                "user.result.conversation.conversationType == 'personal'",
                "user.result.conversation.id == 'a:oneOnOneConversationId'",
                "user.result.conversation.isGroup == false",
                "user.result.conversation.name == 'oneOnOne'",
                "user.result.conversation.tenantId == 'tenantId-Guid'",
                "user.result.meeting.role == 'Organizer'",
                "user.result.user.userPrincipalName == 'userPrincipalName-1'"
            ]
        }
    ]
}