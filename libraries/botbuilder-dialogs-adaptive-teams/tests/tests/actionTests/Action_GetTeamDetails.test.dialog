{
    "$schema": "../../../tests.schema",
    "$kind": "Microsoft.Test.Script",
    "dialog": {
        "$kind": "Microsoft.AdaptiveDialog",
        "id": "planningTest",
        "triggers": [
            {
                "$kind": "Microsoft.OnBeginDialog",
                "actions": [
                  {
                      "$kind": "Teams.GetTeamDetails",
                      "property": "user.response"
                  },
                  {
                    "$kind": "Microsoft.SendActivity",
                    "activity": "${user.response}"
                  }
                ]
            },
            {
                "$kind": "Microsoft.OnError",
                "actions": [
                  {
                    "$kind": "Microsoft.SendActivity",
                    "activity": "${turn}"
                  }
                ]
            }
        ]
    },
    "script": [
        {
            "$kind": "Microsoft.Test.UserActivity",
            "activity": {
                "type": "message",
                "text": "hi",
                "channelData": {
                    "tenant":{
                      "id":"tenant-id-1"
                    },
                    "team": {
                      "id": "team-id-1"
                    }
                }
            }
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "{\"id\":\"19:generalChannelIdgeneralChannelId@thread.skype\",\"name\":\"TeamName\",\"aadGroupId\":\"Team-aadGroupId\"}"
        }
    ]
}