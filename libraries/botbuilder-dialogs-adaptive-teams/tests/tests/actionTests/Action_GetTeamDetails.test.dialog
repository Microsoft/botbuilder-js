{
    "$schema": "../../../../botbuilder-dialogs-adaptive/tests/schema/test.schema",
    "$kind": "Microsoft.Test.Script",
    "enableTrace": true,
    "dialog": {
        "$kind": "Microsoft.AdaptiveDialog",
        "id": "planningTest",
        "triggers": [
            {
                "$kind": "Microsoft.OnUnknownIntent",
                "actions": [
                    {
                        "$kind": "Teams.GetTeamDetails",
                        "property": "$result"
                    },
                    {
                        "$kind": "Microsoft.TraceActivity",
                        "name": "sendResult",
                        "valueType": "memory",
                        "value": "=$result"
                    }
                ]
            }
        ]
    },
    "script": [
        {
            "$kind": "Microsoft.Test.UserActivity",
            "activity": {
                "type": "message",
                "text": "hi",
                "channelData": {
                    "tenant": {
                        "id": "tenant-id-1"
                    },
                    "team": {
                        "id": "team-id-1"
                    }
                }
            }
        },
        {
            "$kind": "Microsoft.Test.AssertReplyActivity",
            "assertions": [
                "value.aadGroupId == 'Team-aadGroupId'",
                "value.id == '19:generalChannelIdgeneralChannelId@thread.skype'",
                "value.name == 'TeamName'"
            ]
        }
    ]
}