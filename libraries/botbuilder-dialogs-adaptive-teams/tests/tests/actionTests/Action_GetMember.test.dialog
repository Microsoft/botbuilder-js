{
    "$schema": "../../../../botbuilder-dialogs-adaptive/tests/schema/test.schema",
    "$kind": "Microsoft.Test.Script",
    "enableTrace": true,
    "dialog": {
        "$kind": "Microsoft.AdaptiveDialog",
        "id": "planningTest",
        "triggers": [
            {
                "$kind": "Microsoft.OnUnknownIntent",
                "actions": [
                    {
                        "$kind": "Teams.GetMember",
                        "property": "$result"
                    },
                    {
                        "$kind": "Microsoft.TraceActivity",
                        "name": "sendResult",
                        "valueType": "memory",
                        "value": "=$result['0']"
                    }
                ]
            }
        ]
    },
    "script": [
        {
            "$kind": "Microsoft.Test.UserActivity",
            "activity": {
                "type": "message",
                "text": "hi",
                "channelData": {
                    "tenant": {
                        "id": "tenant-id-1"
                    },
                    "meeting": {
                        "id": "meeting-id-1"
                    }
                }
            }
        },
        {
            "$kind": "Microsoft.Test.AssertReplyActivity",
            "assertions": [
                "value.email == 'User.0@microsoft.com'",
                "value.givenName == 'User'",
                "value.id == '29:User-Id-0'",
                "value.name == 'getTeamsUser-0'",
                "value.objectId == 'User-0-Object-Id'",
                "value.surname == 0",
                "value.tenantId == 'tenant-id-1'",
                "value.userPrincipalName == 'user0@microsoft.com'"
            ]
        }
    ]
}