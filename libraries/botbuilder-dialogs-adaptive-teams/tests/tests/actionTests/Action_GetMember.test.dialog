{
    "$schema": "../../../../botbuilder-dialogs-adaptive/tests/schema/test.schema",
    "$kind": "Microsoft.Test.Script",
    "dialog": {
        "$kind": "Microsoft.AdaptiveDialog",
        "id": "planningTest",
        "triggers": [
            {
                "$kind": "Microsoft.OnBeginDialog",
                "actions": [
                    {
                        "$kind": "Teams.GetMember",
                        "property": "user.result"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${user.result}"
                    }
                ]
            },
            {
                "$kind": "Microsoft.OnError",
                "actions": [
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${turn.dialogEvent.value.message}"
                    }
                ]
            }
        ]
    },
    "script": [
        {
            "$kind": "Microsoft.Test.UserActivity",
            "activity": {
                "type": "message",
                "text": "hi",
                "channelData": {
                    "tenant": {
                        "id": "tenant-id-1"
                    },
                    "meeting": {
                        "id": "meeting-id-1"
                    }
                }
            }
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "{\"0\":{\"id\":\"29:User-Id-0\",\"name\":\"getTeamsUser-0\",\"objectId\":\"User-0-Object-Id\",\"givenName\":\"User\",\"surname\":0,\"email\":\"User.0@microsoft.com\",\"userPrincipalName\":\"user0@microsoft.com\",\"tenantId\":\"tenant-id-1\"}}"
        }
    ]
}