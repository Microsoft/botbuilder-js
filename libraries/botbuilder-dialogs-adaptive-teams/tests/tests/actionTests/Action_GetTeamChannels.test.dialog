{
    "$schema": "../../../../botbuilder-dialogs-adaptive/tests/schema/test.schema",
    "$kind": "Microsoft.Test.Script",
    "enableTrace": true,
    "dialog": {
        "$kind": "Microsoft.AdaptiveDialog",
        "id": "planningTest",
        "triggers": [
            {
                "$kind": "Microsoft.OnUnknownIntent",
                "actions": [
                    {
                        "$kind": "Teams.GetTeamChannels",
                        "property": "$result"
                    },
                    {
                        "$kind": "Microsoft.TraceActivity",
                        "name": "sendResult",
                        "valueType": "memory",
                        "value": "=$result"
                    }
                ]
            }
        ]
    },
    "script": [
        {
            "$kind": "Microsoft.Test.UserActivity",
            "activity": {
                "type": "message",
                "text": "hi",
                "channelData": {
                    "tenant": {
                        "id": "tenant-id-1"
                    },
                    "team": {
                        "id": "team-id-1"
                    }
                }
            }
        },
        {
            "$kind": "Microsoft.Test.AssertReplyActivity",
            "assertions": [
                "value[0].id == '19:ChannelIdgeneralChannelId@thread.skype'",
                "value[0].name == 'Testing0'",
                "value[1].id == '19:somechannelId2e5ab3df9ae9b594bdb@thread.skype'",
                "value[1].name == 'Testing1'",
                "value[2].id == '19:somechannelId388ade16aa4dd375e69@thread.skype'",
                "value[2].name == 'Testing2'"
            ]
        }
    ]
}