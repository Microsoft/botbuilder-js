{
    "$schema": "../../../../botbuilder-dialogs-adaptive/tests/schema/test.schema",
    "$kind": "Microsoft.Test.Script",
    "dialog": {
        "$kind": "Microsoft.AdaptiveDialog",
        "id": "planningTest",
        "triggers": [
            {
                "$kind": "Microsoft.OnBeginDialog",
                "actions": [
                    {
                        "$kind": "Teams.GetTeamChannels",
                        "property": "user.response"
                    },
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${user.response}"
                    }
                ]
            },
            {
                "$kind": "Microsoft.OnError",
                "actions": [
                    {
                        "$kind": "Microsoft.SendActivity",
                        "activity": "${turn.dialogEvent.value.message}"
                    }
                ]
            }
        ]
    },
    "script": [
        {
            "$kind": "Microsoft.Test.UserActivity",
            "activity": {
                "type": "message",
                "text": "hi",
                "channelData": {
                    "tenant": {
                        "id": "tenant-id-1"
                    },
                    "team": {
                        "id": "team-id-1"
                    }
                }
            }
        },
        {
            "$kind": "Microsoft.Test.AssertReply",
            "text": "[{\"id\":\"19:ChannelIdgeneralChannelId@thread.skype\"},{\"id\":\"19:somechannelId2e5ab3df9ae9b594bdb@thread.skype\",\"name\":\"Testing1\"},{\"id\":\"19:somechannelId388ade16aa4dd375e69@thread.skype\",\"name\":\"Testing2\"}]"
        }
    ]
}