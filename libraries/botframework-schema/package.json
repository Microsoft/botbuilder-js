  "repository": {
    "type": "git",
    "url": "git@github.com:Microsoft/botbuilder-js.git"
  },
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.7.12",
    "typescript": "3.5.3",
    "mocha": "^5.2.0",
    "nyc": "^15.0.0"
  },
  "scripts": {
    "build": "tsc",
    "build:rollup": "npm run clean && npm run build && api-extractor run --verbose --local",
